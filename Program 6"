#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>


struct Student{
    char name[20];
    int age;
    int total_marks;    
};
int main() 
{
struct Student s[10];
int i,member;
scanf("%d",&member);
for(i=0;i<member;i++)
{
scanf("%s",s[i].name);
scanf("%d",&s[i].age);
scanf("%d",&s[i].total_marks);
}
for(i=0;i<member;i++){
    printf("Name: %s\n",s[i].name);
     printf("Age: %d\n",s[i].age);
      printf("Total Marks: %d\n",s[i].total_marks);
      if(i<1){
          printf("\n");
      }
}
return 0;
}
